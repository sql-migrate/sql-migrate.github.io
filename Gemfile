source 'https://rubygems.org'

gem 'jekyll', '~> 4.3'
gem 'base64'
gem 'csv'
gem 'bigdecimal'

# Windows timezone support
gem 'tzinfo'
gem 'tzinfo-data', platforms: [:mingw, :mswin, :x64_mingw, :jruby]

group :jekyll_plugins do
  gem 'jekyll-archives'
  gem 'jekyll-feed'
  gem 'jekyll-paginate'
  gem 'jekyll-seo-tag'
  gem 'jekyll-sitemap'
end
